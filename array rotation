//C program to rotate an array by 
// d elements 
#include <stdio.h> 
  
/* Function to left Rotate array[] of size n by 1*/
void leftRotatebyOne(int array[], int n); 
  
/*Function to left rotate array[] of size n by d*/
void leftRotate(int array[], int d, int n) 
{ 
    int i; 
    for (i = 0; i < d; i++) 
        leftRotatebyOne(array, n); 
} 
  
void leftRotatebyOne(int array[], int n) 
{ 
    int temp = array[0], i; 
    for (i = 0; i < n - 1; i++) 
        array[i] = array[i + 1]; 
    array[i] = temp; 
} 
  
/* utility function to print an array */
void printArray(int array[], int n) 
{ 
    int i; 
    for (i = 0; i < n; i++) 
        printf("%d ", array[i]); 
} 
  
/* Driver program to test above functions */
int main() 
{ 
    int array[] = { 1, 2, 3, 4, 5, 6, 7 }; 
    leftRotate(array, 2, 7); 
    printArray(array, 7); 
    return 0; 
} 
